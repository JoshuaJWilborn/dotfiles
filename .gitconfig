[user]
	name = Joshua Wilborn
	email = jwilborn@extron.com
[alias]
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
[merge]
  tool = vimdiff
[mergetool]
  prompt = true
[mergetool "vimdiff"]
  cmd = nvim -d $BASE $LOCAL $REMOTE $MERGED -c '$wincmd w' -c 'wincmd J'
[core]
	editor = vim
[credential]
  helper = store
